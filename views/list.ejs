<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%=listTitle%></h1>
    </div>
    <div class="box">
            <% for(var i=0; i<item.length;i++) {%>
                <div class="item">
                    <input type="checkbox">
                    <p> <%= item[i] %> </p>
                </div>
            <% } %>
    
            <form action="/" method="POST">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value="<%=listTitle %>">+</button>
            </form>
    </div>
    
    
<%- include("footer.ejs") -%>

